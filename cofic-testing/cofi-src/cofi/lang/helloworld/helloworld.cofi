module .cofi.lang;
package .cofi.lang.helloworld;

import .cofi.lang.collect.ArrayList;
import .cofi.lang.collect.ImmutableList;

public package {}

public object HelloWorld extends Application {
    public id<I, O | I <: O>(value : I) : O { return (value.get()); }

    public start(args : ImmutableList<String>) : () {
        xs : ArrayList<String> := this.id(ArrayList.create(100));
        
        xs.get().append("abc");
        
        i : Natural := 0;
        Bool.while({ -> return (i.get().lessThan(10)); }, { ->
            i := i.get().plus(1);
            xs.get().append(i.get().asString());
        });
        
        xs.get().append("xyz");

        Console.println("Testing… 1, 2, 3… Testing… 1, 2, 3…");

        xs.get().forEach(Console.println);
    }
}
